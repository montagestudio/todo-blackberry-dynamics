<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kaazing.todobbd" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Todo BlackBerry Dynamics</name>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-bbd-base" spec="file:../../plugins/cordova-plugin-bbd-base" />
    <plugin name="cordova-plugin-bbd-sqlite" spec="file:../../plugins/cordova-plugin-bbd-sqlite" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-bbd-configure" spec="file:../../plugins/cordova-plugin-bbd-configure" />
    <engine name="ios" spec="~4.5.1" />
</widget>
